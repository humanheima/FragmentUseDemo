<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hm.fragmentusedemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".modu.ModuActivity"
            android:launchMode="singleTask"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ViewPagerWithFragmentActivity" />
        <activity android:name=".simple.SimpleActivity" />
        <activity android:name=".simple.AnotherActivity" />
        <activity
            android:name=".viewpager.ViewPagerPagerAdapterActivity"

            android:screenOrientation="portrait"></activity>
        <activity android:name=".simple.DemoActivity" />
        <activity android:name=".lazyload.LazyLoadActivity" />
        <activity android:name=".activity.StartActivity"></activity>
        <activity android:name=".activity.KotlinTestActivity"></activity>
    </application>

</manifest>